{
	"name": "bot6.0",
	"description": "Hello World ExpressJS WebService",
	"version": "0.0.1",
	"private": true,
	"dependencies": {
		"aws-lambda": "^1.0.7",
		"aws-serverless-express": "3.1.3",
		"express": "4.16.2"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.101",
		"@types/aws-serverless-express": "^3.3.5",
		"@types/express": "^4.17.13",
		"@types/node": "^18.0.0",
		"concurrently": "^7.2.2",
		"mocha": "5.0.0",
		"supertest": "3.0.0",
		"typescript": "^4.7.4",
		"unit.js": "2.0.0"
	},
	"scripts": {
		"test": "mocha --recursive tests",
		"build": "tsc",
		"watch": "tsc -w --preserveWatchOutput",
		"sam": "sam local start-api --warm-containers LAZY",
		"dev": "concurrently --kill-others \"npm run sam\" \"npm run watch\""
	}
}
